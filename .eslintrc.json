{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "consistent-return": "warn",
    "no-unused-vars": "warn",
    "import/no-unresolved": "off",
    "no-console": "warn",
    "import/extensions": ["off"], // import 시 확장자를 붙이면 나오는 오류 삭제
    "no-plusplus": "off",
    "class-methods-use-this": "off",
    "no-alert": "off",
    "no-underscore-dangle": "off",
    "no-case-declarations": "off"
  }
}
